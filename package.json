{
  "name": "ati-fp-ts",
  "version": "0.1.0",
  "private": true,
  "homepage": "wordpress/wp-content/plugins/pbh-fp-admin",
  "dependencies": {
    "@types/jest": "24.0.11",
    "@types/node": "11.12.2",
    "@types/react": "16.8.10",
    "@types/react-dom": "16.8.3",
    "@types/react-native": "^0.57.42",
    "@types/react-router-dom": "^4.3.3",
    "faker": "^4.1.0",
    "modal-react-native-web": "^0.2.0",
    "nuka-carousel": "^4.5.3",
    "react": "^16.8.6",
    "react-art": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-icons": "^3.5.0",
    "react-native-floating-action": "^1.14.2",
    "react-native-web": "^0.11.2",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8",
    "typescript": "3.4.1"
  },
  "scripts": {
    "preinstall": "rm -f node_modules/@",
    "postinstall": "ln -s `pwd`/src node_modules/@",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "pre-build": "cp public/index.html public/_index.html && cp public/root-partial.html public/index.html",
    "post-build": "cp public/index.html public/root-partial.html && cp public/_index.html public/index.html && ./phpify-fp.sh",
    "pre-build2": "npm run pre-build ; NODE_ENV=dev react-scripts build ; npm run post-build",
    "admin-build": "npm run pre-build2 && rm -rf pbh-fp-admin && mv build pbh-fp-admin && cp admin.php pbh-fp-admin/index.php",
    "admin-deploy": "tar cvzf /tmp/pbh-fp-admin.tgz pbh-fp-admin && scp /tmp/pbh-fp-admin.tgz pbh:/tmp"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
