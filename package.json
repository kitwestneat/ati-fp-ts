{
  "name": "ati-fp-ts",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://example.org/wordpress/wp-content/themes/ati-2018/frontpage",
  "dependencies": {
    "@types/faker": "^5.5.8",
    "@types/jest": "27.0.1",
    "@types/lodash": "^4.14.172",
    "@types/node": "16.6.1",
    "@types/react": "17.0.18",
    "@types/react-dom": "17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "eslint-plugin-import": "^2.24.0",
    "faker": "^5.5.3",
    "lodash": "^4.17.21",
    "modal-react-native-web": "^0.2.0",
    "nuka-carousel": "^4.7.9",
    "react": "^17.0.2",
    "react-art": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-native": "^0.65.0",
    "react-native-floating-action": "^1.22.0",
    "react-native-web": "^0.18.7",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "typescript": "4.3.5"
  },
  "scripts": {
    "preinstall": "rm -f node_modules/@",
    "postinstall": "ln -s `pwd`/src node_modules/@",
    "start": "ln -fs src/index-app.tsx src/index.tsx && react-scripts --openssl-legacy-provider start",
    "test": "react-scripts --openssl-legacy-provider test",
    "eject": "react-scripts --openssl-legacy-provider eject",
    "pre-build": "cp public/index.html public/_index.html && cp public/root-partial.html public/index.html",
    "post-build": "cp public/index.html public/root-partial.html && cp public/_index.html public/index.html && ./phpify-fp.sh",
    "build": "npm run pre-build ; ln -fs src/index-app.tsx src/index.tsx ; NODE_ENV=production react-scripts --openssl-legacy-provider build ; npm run post-build",
    "deploy": "tar cvzf /tmp/front.tgz build && scp /tmp/front.tgz be0:/tmp",
    "admin-start": "ln -fs src/index-admin.tsx src/index.tsx ; react-scripts --openssl-legacy-provider start",
    "admin-build": "ln -fs src/index-admin.tsx src/index.tsx; PUBLIC_URL=https://example.org/wordpress/wp-content/plugins/pbh-fp-admin npm run build && rm -rf pbh-fp-admin && mv build pbh-fp-admin && cp src/admin-php/*.php pbh-fp-admin",
    "admin-deploy": "tar cvzf /tmp/pbh-fp-admin.tgz pbh-fp-admin && scp /tmp/pbh-fp-admin.tgz be0:/tmp"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/react-native": "^0.64.13",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/eslint-plugin-tslint": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "codegen.macro": "^4.1.0",
    "eslint-config-google": "^0.14.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^5.0.0"
  }
}
